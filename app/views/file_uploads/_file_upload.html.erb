<tr id="<%= dom_id file_upload %>">
  <td><%= file_upload.title %></td>
  <td><%= file_upload.description %></td>
  <td><%= file_upload.share_key %></td>
  <td><%= file_upload.file %></td>
  <td>
    <%= link_to "Show", file_upload, class: "btn btn-info btn-sm" %>
    <% if file_upload.file.attached? %>
      <%= link_to "Download", rails_blob_path(file_upload.file, disposition: "attachment"), class: "btn btn-success btn-sm" %>
    <% end %>
    <%#= link_to "Delete", file_upload, method: :delete, data: { confirm: "Are you sure you want to delete this file?" }, class: "btn btn-danger btn-sm" %>
    <%= button_to "Destroy this file upload", @file_upload, method: :delete %>
    <%= button_to "Generate Token", generate_token_file_upload_path(file_upload), method: :post, class: "btn btn-warning btn-sm" %>
    <%#= link_to "Share", share_file_url(file_upload.public_token), target: "_blank" %>
  </td>
</tr>